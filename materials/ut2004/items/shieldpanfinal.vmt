"UnlitGeneric"
{
	"$basetexture" "ut2004/items/SuperShieldPan"
	"$additive" "1"
	"$nocull" "1"
	
	"$translate" "[0 0]"
	"$center" "[0.5 0.5]"
	
	"Proxies"
	{
		"LinearRamp"
		{
		"rate" -1
		"initialValue" 0.0
		"resultVar" "$translate[0]"
		}

		"TextureTransform"
		{
		"translateVar" "$translate"
		"centerVar" "$center"
		"resultVar" "$basetexturetransform"
		}
	}
}
