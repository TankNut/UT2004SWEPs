"UnlitGeneric"
{
	"$basetexture" "ut2004/effects/shield_elec_green_a"
	"$additive" 1
	"$translucent" 1
	"$nocull" 1
	
	"$translate" "[0 0]"
	"$center" "[0.5 0.5]"
	
	"Proxies"
	{
		"LinearRamp"
		{
		"rate" 0.2
		"initialValue" 0.0
		"resultVar" "$translate[0]"
		}

		"TextureTransform"
		{
		"translateVar" "$translate"
		"centerVar" "$center"
		"resultVar" "$basetexturetransform"
		}
	}
}
